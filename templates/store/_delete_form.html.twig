<form method="post" action="{{ path('app_store_delete', {'id': store.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ store.id) }}">
	<button class="btn btn-danger me-2 mt-2">Supprimer</button>
</form>
